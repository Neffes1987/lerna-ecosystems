  "main": "webpack.config.babel.js",
  "scripts": {
    "build": "webpack --config webpack.config.babel.js",
    "profile": "webpack --profile --json --config webpack.config.babel.js > ./dist/profile.json && webpack-bundle-analyzer ./dist/profile.json",
    "start": "webpack serve --config webpack.config.babel.js",
    "test": "jest --maxWorkers=50%",
    "test:watch": "npm run build:messages && jest --watch",
    "lerna:check-dep": "lerna run checkDep --stream --no-bail --exclude-dependents",
    "lerna:check-dep:current": "lerna run checkDep --stream --no-bail --since HEAD --exclude-dependents -- --fix",
    "lerna:check-dep:autofix": "lerna run checkDep --stream --no-bail --exclude-dependents -- --fix",
    "lint:types": "tsc --noEmit -p tsconfig.json",
    "lint:eslint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "lint:prettier": "prettier --write \"src/modules/**/*.(ts|tsx)\"",
    "lint": "yarn lint:eslint && yarn lint:types",
    "lerna:clean:build": "find . -name 'build' -type d -prune -exec rm -rf '{}' +",
    "lerna:build:all": "yarn run lerna run build --include-dependencies --stream",
    "generate:package": "lerna-scripts generate:package",
    "lerna:publish": "lerna-scripts publish:manually",
    "prepack:packages": "lerna run pack --stream",
    "postpack:packages": "lerna run revertPack --stream",
    "clean-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && yarn run lerna:clean:build "
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "prettier --write",
      "eslint --fix --quiet --cache"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint:types"
    }
  },
  "private": true,
  "workspaces": [
    "tools/*",
    "src/modules/*",
    "src/modules/*/*"
  ],
  "dependencies": {
    "@components/modulea": "0.0.1",
